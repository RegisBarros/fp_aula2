@model IEnumerable<Fiap2.Core.Photo>
@{
    ViewData["Title"] = "Home Page";
}

<h1>@ViewBag.Category</h1>

<ul>
    @foreach (var photo in Model)
    {
        <li>
            <a href="@photo.Url" target="_blank">
                <img src="@photo.ThumbnailUrl" alt="@photo.Title" />
            </a>
        </li>
    }
</ul>
